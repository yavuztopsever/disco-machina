# Server Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=info
API_PREFIX=/api/v1

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Security
JWT_SECRET=change-this-in-production
JWT_EXPIRES_IN=1d

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Cache
CACHE_TTL=60
CACHE_CHECK_PERIOD=120